$(function(){
	$('#delete_detail_final').slideUp(20, function(){$('#order_detail_outline').remove();});
	$("#item_<%= @detail.itemID %>").removeClass('selected').html("<%= escape_javascript(render 'pages/item', :item => @detail.item, :@order => @detail.order) %>");
	$("#group_<%= @detail.groupID %>").html("<%= escape_javascript(render 'pages/group', :group => @detail.group) %>");
	$("#chef_special_header").html("<%= escape_javascript(render 'pages/chef_special_header', :type => @detail.type) %>");
});